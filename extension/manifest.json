{
  "manifest_version": 3,
  "name": "Browser Sniffer",
  "version": "0.1.0",
  "description": "Capture network activity via Chrome DevTools Protocol",
  "permissions": ["debugger", "tabs", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
